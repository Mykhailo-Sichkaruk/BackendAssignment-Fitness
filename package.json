{
  "name": "fitnessapp",
  "version": "1.0.0",
  "description": "Testing assigments for Backend developer interview",
  "main": "src/index.ts",
  "type": "module",
  "target": "es2022",
  "module": "es2022",
  "scripts": {
    "compile": "tsc",
    "start": "npm run db-dev && node src/index.js",
    "db-generate": "npx prisma generate --schema=./src/infrastructure/prisma/schema.prisma",
    "db-format": "npx prisma format --schema=./src/infrastructure/prisma/schema.prisma",
    "db-dev": "npx prisma db push --skip-generate --schema=./src/infrastructure/prisma/schema.prisma",
    "db-migrate": "npx prisma migrate deploy --schema=./src/infrastructure/prisma/schema.prisma"
  },
  "author": "richard.solar@goodrequest.com",
  "license": "ISC",
  "dependencies": {
    "@goodrequest/passport-jwt-wrapper": "^1.7.1",
    "@prisma/client": "^5.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "body-parser": "^1.20.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-metarhia": "^8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.6",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.4.1",
    "prettier": "^3.0.3",
    "prisma": "^5.2.0",
    "sequelize": "^6.33.0",
    "swagger-ui-express": "^5.0.0",
    "ts-dotenv": "^0.9.1",
    "ts-node": "^10.9.1",
    "ts-results-es": "^4.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "eslint-plugin-security": "^1.7.1"
  }
}
